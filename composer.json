{
    "name": "hiqdev/yii2-language",
    "type": "yii2-extension",
    "description": "Yii2 module for language switching",
    "keywords": [
        "yii2",
        "extenstion",
        "language",
        "switcher",
        "module",
        "widget"
    ],
    "homepage": "https://github.com/hiqdev/yii2-language",
    "license": "BSD-3-Clause",
    "support": {
        "email": "support@hiqdev.com",
        "source": "https://github.com/hiqdev/yii2-language",
        "issues": "https://github.com/hiqdev/yii2-language/issues",
        "wiki": "https://github.com/hiqdev/yii2-language/wiki",
        "forum": "http://forum.hiqdev.com/"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "role": "Project lead",
            "email": "sol@hiqdev.com",
            "homepage": "http://hipanel.com/"
        },
        {
            "name": "Dmitry Naumenko",
            "role": "Lead backend developer",
            "email": "d.naumenko.a@gmail.com",
            "homepage": "http://silverfire.me/"
        },
        {
            "name": "Andrey Klochok",
            "role": "Lead frontend developer",
            "email": "andreyklochok@gmail.com",
            "homepage": "http://hiqdev.com/"
        },
        {
            "name": "Yuriy Myronchuk",
            "role": "QA Lead",
            "email": "bladeroot@gmail.com",
            "homepage": "http://hiqdev.com/"
        }
    ],
    "require": {
        "hiqdev/yii2-menus": "*@dev",
        "hiqdev/yii2-thememanager": "*@dev"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "hiqdev/hidev-hiqdev": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\yii2\\language\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hiqdev\\yii2\\language\\tests\\": "tests"
        }
    },
    "extra": {
        "config-plugin": {
            "params": "config/params.php",
            "web": "config/web.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
